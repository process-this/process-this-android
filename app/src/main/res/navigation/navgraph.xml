<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/navgraph"

  app:startDestination="@id/navigation_home">
  <fragment
    android:id="@+id/navigation_home"
    android:name="io.github.processthis.client.view.HomeFragment"
    android:label="HomeFragment"/>

  <fragment
    android:id="@+id/navigation_userprofile"
    android:label="userProfileFragment"
    android:name="io.github.processthis.client.view.UserProfileFragment"
    />

  <fragment
    android:id="@+id/navigation_featured"
    android:label="featuredFragment"
    android:name="io.github.processthis.client.view.FeaturedFragment"
    tools:layout="@layout/fragment_featured"/>

  <fragment
    android:id="@+id/navigation_sketch_view"
    android:label="sketchView"
    android:name="io.github.processthis.client.view.SketchEditorFragment"
    tools:layout="@layout/ide_fragment"/>


</navigation>